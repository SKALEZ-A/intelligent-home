PORT=3011
NODE_ENV=development

# JWT
JWT_SECRET=your_jwt_secret_key_here

# Speech Recognition
SPEECH_RECOGNITION_PROVIDER=google
GOOGLE_SPEECH_API_KEY=your_google_speech_api_key
AZURE_SPEECH_KEY=your_azure_speech_key
AZURE_SPEECH_REGION=eastus

# Natural Language Processing
NLP_PROVIDER=dialogflow
DIALOGFLOW_PROJECT_ID=your_dialogflow_project_id
DIALOGFLOW_PRIVATE_KEY=your_dialogflow_private_key
DIALOGFLOW_CLIENT_EMAIL=your_dialogflow_client_email

# Text-to-Speech
TTS_PROVIDER=google
GOOGLE_TTS_API_KEY=your_google_tts_api_key

# Voice Profiles
VOICE_PROFILE_ENABLED=true
VOICE_RECOGNITION_CONFIDENCE=0.8

# Command Execution
COMMAND_TIMEOUT_MS=5000
MAX_RETRY_ATTEMPTS=3

# Logging
LOG_LEVEL=info
