PORT=3004
NODE_ENV=development

# MongoDB
MONGODB_URI=mongodb://localhost:27017/automations

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# JWT
JWT_SECRET=your_jwt_secret_key_here

# Automation Engine
MAX_CONCURRENT_EXECUTIONS=10
EXECUTION_TIMEOUT_MS=30000
RETRY_ATTEMPTS=3

# Scheduler
SCHEDULER_ENABLED=true
SCHEDULER_INTERVAL=60000

# Machine Learning
ML_SERVICE_URL=http://localhost:5000
ML_ENABLED=true

# Geofencing
GEOFENCE_CHECK_INTERVAL=30000

# Logging
LOG_LEVEL=info
